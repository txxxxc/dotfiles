{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.formatOnTypeFiletypes": ["go"],
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "trace.server": "verbose",
      "filetypes": ["typescript", "typescriptreact", "graphql"],
      "settings": {
        "graphql-config.load.legacy": true
      }
    }
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescriptreact",
    "typescript",
    "vue"
  ],
  "javascript.suggestionActions.enabled": false,
  "tsserver.formatOnType": false,
  "tsserver.trace.server": "verbose",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.autoFixOnFormat": true,
  "stylelintplus.validateOnSave": true,
  "jedi.enable": true,
  "jedi.executable.command": "~/.anyenv/envs/pyenv/shims/jedi-language-server",
  "prettier.disableLanguages": ["cpp"]
}
